{
  "name": "react-boilerplate",
  "version": "3.4.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3.10",
    "node": ">=6.10"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "ts-node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.ts.js --color -p --progress",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "ts-node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development ts-node server/index.js",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true ts-node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production ts-node server/index.js",
    "presetup": "npm i chalk shelljs",
    "setup": "ts-node ./internals/scripts/set---up.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "(npm run lint:ts || true) && npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:tslint": "tslint 'app/**/*.ts' 'app/**/*.tsx'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:ts": "npm run lint:tslint",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node",
          "react-hot-loader/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        0,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0,
      "import/extensions": 0,
      "semi": [
        1,
        "never"
      ],
      "quotes": [
        1,
        "double",
        "avoid-escape"
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.ts.js"
        }
      }
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}",
      "!app/**/*.test.{js,jsx,ts,tsx}",
      "!app/*/RbGenerated*/*.{js,jsx,ts,tsx}",
      "!app/app.{js,jsx,ts,tsx}",
      "!app/routes.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest"
    },
    "testRegex": "tests/.*\\.test\\.(js|jsx|ts|tsx)$"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cross-env": "^4.0.0",
    "express": "^4.15.2",
    "fontfaceobserver": "^2.0.9",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "invariant": "^2.2.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "minimist": "^1.2.0",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.5.1",
    "react-dom": "^15.4.2",
    "react-helmet": "^3.2.2",
    "react-intl": "^2.1.5",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.3",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-immutable": "^4.0.0",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.14.4",
    "reselect": "^3.0.0",
    "sanitize.css": "^5.0.0",
    "styled-components": "^1.4.4",
    "tslib": "^1.6.0",
    "warning": "^3.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/chalk": "^0.4.31",
    "@types/cheerio": "^0.22.1",
    "@types/classnames": "0.0.32",
    "@types/compression": "0.0.33",
    "@types/enzyme": "^2.7.6",
    "@types/express": "^4.0.35",
    "@types/fontfaceobserver": "0.0.5",
    "@types/html-webpack-plugin": "^2.22.4",
    "@types/immutable": "^3.8.7",
    "@types/invariant": "^2.2.28",
    "@types/ip": "0.0.29",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.61",
    "@types/loglevel": "^1.4.29",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.40",
    "@types/react": "^15.0.20",
    "@types/react-addons-create-fragment": "^0.14.16",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-dom": "^0.14.23",
    "@types/react-helmet": "0.0.28",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-intl": "^2.2.5",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^3.0.10",
    "@types/react-router-redux": "^4.0.44",
    "@types/redux": "^3.6.31",
    "@types/redux-form": "^6.3.6",
    "@types/redux-immutable": "^3.0.33",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.0.27",
    "@types/rimraf": "0.0.28",
    "@types/shelljs": "^0.7.0",
    "@types/sinon": "^2.1.0",
    "@types/warning": "^3.0.0",
    "@types/webpack": "^2.2.14",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-hot-middleware": "^2.15.0",
    "@types/whatwg-fetch": "0.0.33",
    "awesome-typescript-loader": "^3.1.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-env": "^1.3.2",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "cheerio": "^0.22.0",
    "circular-dependency-plugin": "^2.0.0",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-redux-saga": "^0.3.0",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.1",
    "gulp-sass": "^3.1.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "imports-loader": "^0.7.1",
    "jest-cli": "^19.0.2",
    "lint-staged": "^3.4.0",
    "ngrok": "^2.2.6",
    "node-gyp": "^3.6.0",
    "node-plop": "^0.5.5",
    "node-sass": "^4.5.2",
    "nodegit": "^0.18.0",
    "null-loader": "^0.1.1",
    "offline-plugin": "^4.6.2",
    "plop": "^1.7.4",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "regenerator-runtime": "^0.10.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "scss-lint": "0.0.0",
    "shelljs": "^0.7.7",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "typescript-babel-jest": "^1.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
